{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "react/prop-types": 0
  },
  "settings": {
    "react": {
      "pragma": "h",
      "version": "16.3"
    }
  }
}
